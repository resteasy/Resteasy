<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
    <deployment>
        <dependencies>
            <module name="org.jboss.resteasy.resteasy-spring-web"/>
            <module name="org.jboss.resteasy.resteasy-json-binding-provider" services="import"/>
            <module name="org.springframework.spring">
                <imports>
                    <include path="META-INF**"/>
                    <include path="org**"/>
                </imports>
            </module>
        </dependencies>
    </deployment>
</jboss-deployment-structure>
